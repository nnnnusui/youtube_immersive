{
	"name": "yt_immersive",
	"service": "workspace",
	"workspaceFolder": "/work",
	"dockerComposeFile": "docker-compose.yml",
	"onCreateCommand": "pnpm install --shamefully-hoist",
	"customizations": {
		"vscode": {
			"extensions": [
				"SamKirkland.plop-templates",
				"dbaeumer.vscode-eslint",
				"sysoev.language-stylus",
				"stylelint.vscode-stylelint",
				"ygkn.storybook-opener",
				"eamodio.gitlens"
			]
		}
	},
	"remoteUser": "node"
}