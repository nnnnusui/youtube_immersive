.App
  min-width 300px

  text-align center

.logo
  animation logo-spin infinite 20s linear
  height 40vmin
  pointer-events none

.header
  min-height 100vh

  background-color #282c34
  color white

  display flex
  flex-direction column
  align-items center
  justify-content center
  font-size calc(10px + 2vmin)

.link
  color #b318f0

@keyframes logo-spin
  from
    transform rotate(0deg)
  to
    transform rotate(360deg)
